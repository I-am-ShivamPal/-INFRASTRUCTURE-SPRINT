# Environment Configuration
ENV=dev

# Development Environment Overrides
DEV_HOST=localhost
DEV_PORT=8501

# Staging Environment Overrides
STAGING_IMAGE=shivam/sampleapp:staging
STAGING_PORT=8501

# Cloud Environment Overrides
CLOUD_SERVICE_NAME=sampleapp
CLOUD_REGION=free

# API Keys and Secrets (DO NOT COMMIT REAL VALUES)
RENDER_API_KEY=your_render_api_key_here
DOCKER_REGISTRY_TOKEN=your_docker_token_here
DATABASE_URL=your_database_url_here

# Monitoring Configuration
MONITOR_INTERVAL=30
SLOW_THRESHOLD=5
AUTOFIX_INTERVAL=60